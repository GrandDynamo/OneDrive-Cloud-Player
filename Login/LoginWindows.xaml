<Window x:Class="OneDrive_Cloud_Player.Login.LoginWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneDrive_Cloud_Player.Login"
        mc:Ignorable="d"
        Title="LoginWindows" Height="450" Width="800">
    <Window.Resources>
        <local:LoginViewModel x:Key="vm"/>
    </Window.Resources>
    <Grid>
        <Button Content="Login" Name="LoginButn" FontSize="30" Command="{Binding MyCommand, Source={StaticResource vm}}"  HorizontalAlignment="Center" VerticalAlignment="Top" Height="51" Width="253" Margin="0,307,0,0"/>
        <Label Content="Please login." FontSize="30" HorizontalAlignment="Center" Margin="0,180,0,0" VerticalAlignment="Top"/>
        <Button Content="Test Token" FontSize="30" Command="{Binding MyCommand2, Source={StaticResource vm}}" HorizontalAlignment="Center" VerticalAlignment="Top" Height="51" Width="253" Margin="0,30,0,0"/>

    </Grid>
</Window>
